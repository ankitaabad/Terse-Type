{
  "name": "terse-type",
  "version": "0.0.4",
  "type": "module",
  "description": "Shorthand to generate types and  validators",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "esbuild esbuild.js",
    "tsup": "tsup"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "keywords": [
    "typescript",
    "short-types",
    "autogenerate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankitaabad/Terse-Type.git"
  },
  "author": "Ankit Aabad <ankit.aabad1@gmail.com> (https://github.com/ankitaabad) ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ankitaabad/Terse-Type/issues"
  },
  "homepage": "https://github.com/ankitaabad/Terse-Type#readme",
  "devDependencies": {
    "esbuild": "^0.24.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
